{
  "name": "weather-app-api",
  "version": "1.0.0",
  "description": "api for weather app",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "cp -r src/* dist",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js --quiet",
    "lint:fix": "npm run lint -- --fix",
    "pre-commit": "lint-staged",
    "prepare": "husky && husky install",
    "start": "node -r dotenv/config src/index.js",
    "test": "npx jest --coverage --passWithNoTests",
    "watch": "nodemon --watch src --exec \"npm run start\""
  },
  "author": "Rachael Smith",
  "license": "MIT",
  "dependencies": {
    "async-lock": "^1.4.1",
    "awilix": "^12.0.3",
    "awilix-express": "^9.0.1",
    "babel-jest": "^29.7.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "express-validator": "^7.2.0",
    "gravatar": "^1.8.2",
    "helmet": "^8.0.0",
    "http-status-codes": "^2.3.0",
    "husky": "^9.1.6",
    "joi": "^17.13.3",
    "joi-to-swagger": "^6.2.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.8.0",
    "mongoose-aggregate-paginate-v2": "^1.1.2",
    "mongoose-paginate-v2": "^1.8.5",
    "pino": "^9.5.0",
    "pino-pretty": "^11.3.0",
    "randexp": "^0.5.3",
    "rate-limit-redis": "^4.2.0",
    "redis": "^4.7.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.2.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@eslint/js": "^9.14.0",
    "eslint": "^9.14.0",
    "eslint-plugin-jest": "^28.9.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  }
}
